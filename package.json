{
  "name": "@julr/tooling-configs",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "ESLint config presets",
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "license": "MIT",
  "keywords": [
    "eslint-config",
    "flat"
  ],
  "exports": {
    "./prettier": "./dist/src/prettier/index.json",
    "./tsconfigs/*": "./dist/src/tsconfigs/*.json",
    "./eslint": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "build": "tsup --format esm,cjs --clean --dts",
    "release": "bumpp package.json packages/*/package.json packages/eslint-configs/*/package.json --commit \"chore(release): %s\" --push --tag && pnpm -r publish --access public"
  },
  "dependencies": {
    "@eslint/eslintrc": "^2.1.3",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@unocss/eslint-config": "^0.57.6",
    "eslint-config-flat-gitignore": "^0.1.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-perfectionist": "^2.4.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "vue-eslint-parser": "^9.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-define-config": "1.23.0-2",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.9.2",
    "bumpp": "^9.2.0",
    "cpy": "^11.0.0",
    "eslint": "^8.54.0",
    "eslint-flat-config-viewer": "^0.1.3",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.2.2"
  },
  "prettier": "./src/prettier/index.json"
}
