{
  "name": "@julr/tooling-configs",
  "type": "module",
  "version": "3.3.0",
  "packageManager": "pnpm@9.15.4",
  "description": "ESLint config presets",
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "license": "MIT",
  "keywords": [
    "eslint-config",
    "flat"
  ],
  "exports": {
    "./prettier": "./dist/src/prettier/index.json",
    "./tsconfigs/*": "./dist/src/tsconfigs/*.json",
    "./eslint": "./dist/eslint/index.js"
  },
  "bin": "./dist/cli/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "build": "tsup",
    "release": "pnpm build && release-it",
    "typecheck": "tsc --noEmit",
    "checks": "pnpm typecheck && pnpm lint"
  },
  "dependencies": {
    "@adonisjs/eslint-plugin": "2.0.0-beta.5",
    "@clack/prompts": "^0.9.1",
    "@stylistic/eslint-plugin": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@unocss/eslint-plugin": "^65.4.3",
    "eslint-config-flat-gitignore": "^2.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-perfectionist": "^4.8.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.0.0",
    "picocolors": "^1.1.1",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.1",
    "cpy": "^11.1.0",
    "eslint": "^9.19.0",
    "eslint-flat-config-utils": "^2.0.1",
    "prettier": "^3.4.2",
    "release-it": "^18.1.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "prettier": "./src/prettier/index.json",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "release ${version}"
    },
    "github": {
      "release": true,
      "web": true
    }
  }
}
